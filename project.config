[project]
	description = Puppet module for managing Apache ZooKeeper
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/sandip-calsoft
[access "refs/for/refs/*"]
	read = group user/sandip-calsoft
	push = group user/sandip-calsoft
	label-Code-Review = -2..+2 group user/sandip-calsoft
	label-Verified = -1..+1 group user/sandip-calsoft
	rebase = group user/sandip-calsoft
	removeReviewer = group user/sandip-calsoft
	submit = group user/sandip-calsoft
[access "refs/heads/*"]
	read = group user/sandip-calsoft
	create = group user/sandip-calsoft
	push = +force group user/sandip-calsoft
	pushMerge = group user/sandip-calsoft
[access "refs/tags/*"]
	push = +force group user/sandip-calsoft
	pushTag = group user/sandip-calsoft
	pushSignedTag = group user/sandip-calsoft
